{
  "name": "@digitalbazaar/security-document-loader",
  "version": "1.1.2-0",
  "description": "A JSON-LD documentLoader library pre-loaded with core commonly used contexts (suites, VC, DIDs).",
  "homepage": "https://github.com/digitalbazaar/security-document-loader",
  "author": {
    "name": "Digital Bazaar, Inc.",
    "email": "support@digitalbazaar.com",
    "url": "https://digitalbazaar.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/digitalbazaar/security-document-loader"
  },
  "bugs": {
    "url": "https://github.com/digitalbazaar/security-document-loader/issues",
    "email": "support@digitalbazaar.com"
  },
  "license": "BSD-3-Clause",
  "files": [
    "lib/*.js"
  ],
  "main": "lib/index.js",
  "module": "lib/main.js",
  "dependencies": {
    "@digitalbazaar/did-io": "^1.0.0",
    "@digitalbazaar/did-method-key": "^2.0.0",
    "credentials-context": "^1.0.0",
    "did-context": "^3.0.1",
    "ed25519-signature-2020-context": "^1.0.1",
    "esm": "^3.2.25",
    "jsonld-document-loader": "^1.2.0",
    "veres-one-context": "^12.0.0",
    "x25519-key-agreement-2020-context": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.17.9",
    "@digitalbazaar/security-context": "^1.0.0",
    "babel-loader": "^8.2.5",
    "chai": "^4.3.6",
    "cit-context": "^2.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "eslint-config-digitalbazaar": "^3.0.0",
    "eslint-plugin-jsdoc": "^39.2.9",
    "karma": "^6.3.19",
    "karma-babel-preprocessor": "^8.0.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
    "mocha": "^9.2.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0",
    "webpack": "^5.72.0"
  },
  "nyc": {
    "exclude": [
      "tests"
    ],
    "reporter": [
      "html",
      "text-summary"
    ]
  },
  "browser": {
    "buffer": false,
    "crypto": false,
    "util": false
  },
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "Decentralized",
    "Linked Data"
  ],
  "scripts": {
    "test": "npm run lint && npm run test-node && npm run test-karma",
    "test-node": "cross-env NODE_ENV=test mocha -r esm --preserve-symlinks -t 10000 test/*.spec.js",
    "test-karma": "karma start karma.conf.js",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary npm run test-node",
    "coverage-ci": "cross-env NODE_ENV=test nyc --reporter=lcovonly npm run test-node",
    "coverage-report": "nyc report",
    "lint": "eslint ."
  }
}
